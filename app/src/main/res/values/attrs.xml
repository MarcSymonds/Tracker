<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="colourPickerChoices" format="reference" />
    <attr name="numColumns" format="integer" />
    <attr name="showDialog" format="boolean" />
    <attr name="colorShape" format="enum">
        <enum name="circle" value="1" />
        <enum name="square" value="2" />
    </attr>
    <attr name="viewSize" format="enum">
        <enum name="normal" value="1" />
        <enum name="large" value="2" />
    </attr>

    <declare-styleable name="ColourPickerPreference">
        <attr name="colourPickerChoices" />
        <attr name="numColumns" />
        <attr name="colorShape" />
        <attr name="viewSize" />
        <attr name="showDialog" />
    </declare-styleable>

    <attr name="minValue" format="integer" />
    <attr name="maxValue" format="integer" />

    <declare-styleable name="ConstrainedIntegerPreference">
        <attr name="minValue" />
        <attr name="maxValue" />
    </declare-styleable>

    <attr name="longClickIconLocation" format="enum">
        <enum name="topLeft" value="1" />
        <enum name="topRight" value="2" />
    </attr>

    <attr name="longClickIconOn" format="reference" />
    <attr name="longClickIconOff" format="reference" />
</resources>